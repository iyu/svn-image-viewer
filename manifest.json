{
  "manifest_version": 2,
  "name": "SVN Image Viewer",
  "version": "1.1.0",

  "default_locale": "ja",
  "description": "svn image viewer",
  "icons": {},

  "page_action": {
    "default_icon": {},
    "default_title": "SVN Image Viewer",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "options_page": "options.html",

  "author": "Yuhei Aihara",
  "homepage_url": "https://github.com/iyu/svn-image-viewer",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
